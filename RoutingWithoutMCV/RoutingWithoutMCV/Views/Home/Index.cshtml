
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>


<h1>Current Date and time is: @DateTime.Now</h1>
<h1>Current year is : @DateTime.Now.Year</h1>

@{
    var name = "Sangam";
    var age = 19;
}

<h2>Name: @name</h2>
<h2>Age: @age</h2>

@{
    var MyName = "Sangam";
    if(MyName.Equals("Sangam")){
        <h2>Hey Sangam</h2>
    }
    else{
        <h2>Hey Panday</h2>
    }
}

@{
    for(int i = 0; i< 10; i++)
    {
        <h2>@i</h2>
    }
}

@{
    string[] names = ["Sangam", "Panday"];
    foreach(var item in names)
    {
        <h2>@item</h2>
    }
}
